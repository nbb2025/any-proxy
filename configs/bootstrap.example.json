{
  "domains": [
    {
      "id": "demo-edge",
      "domain": "demo.any-proxy.local",
      "enableTls": false,
      "upstreams": [
        {
          "address": "10.0.10.15:8080",
          "weight": 1
        },
        {
          "address": "10.0.10.16:8080",
          "weight": 1,
          "usePersistent": true
        }
      ],
      "edgeNodes": ["edge-a", "edge-b"],
      "metadata": {
        "sticky": true,
        "timeoutProxy":  "5s",
        "timeoutRead":   "30s",
        "timeoutSend":   "30s"
      }
    }
  ],
  "tunnels": [
    {
      "id": "ssh-demo",
      "groupId": "tg-default",
      "protocol": "tcp",
      "bindHost": "0.0.0.0",
      "bindPort": 2222,
      "bridgeBind": "127.0.0.1",
      "bridgePort": 6200,
      "target": "home-ssh",
      "nodeIds": ["edge-a"],
      "idleTimeout": "2m",
      "metadata": {
        "enableProxyProtocol": false,
        "description": "SSH relay"
      }
    }
  ],
  "tunnelGroups": [
    {
      "id": "tg-default",
      "name": "Default Tunnel Edge Group",
      "listenAddress": "127.0.0.1:4433",
      "edgeNodeIds": ["edge-a"]
    }
  ],
  "tunnelAgents": [
    {
      "id": "agent-home",
      "nodeId": "home-tunnel-node",
      "groupId": "tg-default",
      "keyHash": "490096647c1a035bace3c297f23fc92af04ae4c9750145dc893b34f2038acc6a",
      "keyVersion": 1,
      "enabled": true,
      "services": [
        {
          "id": "home-ssh",
          "protocol": "tcp",
          "localAddress": "127.0.0.1",
          "localPort": 22,
          "remotePort": 2222
        }
      ]
    }
  ]
}
